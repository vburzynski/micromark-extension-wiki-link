    "eslint-plugin-mocha": "^10.4.3",
    "micromark": "^4.0.0",
    "micromark-extension-gfm-table": "^2.0.0",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "proper-tags": "^2.0.2",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.14"
  },
  "dependencies": {
    "micromark-util-character": "^2.1.0",
